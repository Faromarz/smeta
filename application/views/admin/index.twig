<!--[if !IE]><!-->
<html lang="ru" class="no-js">
    <!--<![endif]-->
    <!-- BEGIN HEAD -->
    <head>
        <meta charset="utf-8"/>
        <title>{{ _title }}</title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
        <meta content="{{ _description }}" name="description"/>
        <meta content="{{ _keywords }}" name="keywords" />
        <meta content="senj" name="author"/>
        <link rel="shortcut icon" href="{{ Url.media('/media/img/favicon.ico') }}" type="image/x-icon"/>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/jquery-1.11.0.min.js') }}" type="text/javascript"></script>
        <!-- BEGIN GLOBAL MANDATORY STYLES -->
        {% block styles %}
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/font-awesome/css/font-awesome.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/simple-line-icons/simple-line-icons.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap/css/bootstrap.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/uniform/css/uniform.default.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/select2/select2.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/data-tables/DT_bootstrap.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap-datepicker/css/datepicker.css') }}">
            
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/css/components.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/global/css/plugins.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/admin/layout/css/layout.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/admin/layout/css/themes/default.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ Url.media('/media/libs/metronic/assets/admin/layout/css/custom.css') }}">
            <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css"/>
            <link rel="stylesheet" type="text/css" href="{{ Url.media('media/libs/metronic/assets/global/plugins/bootstrap-editable/bootstrap-editable/css/bootstrap-editable.css')}}"/>
            <link href="{{ Url.media('/media/css/admin/dropzone.css') }}" rel="stylesheet" type="text/css"/>
        {% endblock styles %}
    </head>
    <body class="page-header-fixed ">
        {% include "admin/head.twig" %}
        <!-- END HEADER -->
        <div class="clearfix"></div>
        <div class="page-container">
        {% include "admin/leftmenu.twig" %}
        <div class="page-content-wrapper">
            <div class="page-content">
                {% block boody %}
                {% endblock boody %}  
            </div>
        </div>
        </div>
        {% include "admin/footer.twig" %}
        
        {% block scripts %}
        <!--[if lt IE 9]>
            <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/respond.min.js') }}" type="text/javascript"></script>
            <script src="{{ Url.media('/assets/global/plugins/excanvas.min.js') }}" type="text/javascript"></script>
            <![endif]-->
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/jquery-migrate-1.2.1.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js') }}" type="text/javascript"></script>
