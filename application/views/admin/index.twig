        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/jquery.cokie.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/uniform/jquery.uniform.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/select2/select2.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/data-tables/jquery.dataTables.min.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/data-tables/DT_bootstrap.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/scripts/metronic.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/admin/layout/scripts/layout.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/scripts/datatable.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/admin/pages/scripts/table-ajax.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/bootstrap-editable/bootstrap-editable/js/bootstrap-editable.min.js')}}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/js/admin/index.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/libs/metronic/assets/global/plugins/dropzone/dropzone.js') }}" type="text/javascript"></script>
        <script src="{{ Url.media('/media/js/admin/form-dropzone.js') }}" type="text/javascript"></script>
        <script>
            jQuery(document).ready(function() {
                Metronic.init(); // init metronic core components
                Layout.init(); // init current layout
                TableAjax.init();
                Admin.init();
                FormDropzone.init();
            });
        </script>
        {% endblock scripts %}
    </body>
</html>